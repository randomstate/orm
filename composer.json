{
  "name": "randomstate/orm",
  "description": "Laravel-Doctrine Integration with Custom Repositories",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Connor Forsyth",
      "email": "connor@randomstate.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "RandomState\\Orm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RandomState\\Orm\\Tests\\" : "tests",
      "Tests\\": "vendor/laravel/laravel/tests"
    }
  },
  "require": {
    "laravel-doctrine/orm": "^1.8.3",
    "laravel-doctrine/extensions": "^1",
    "laravel-doctrine/fluent": "^1.2.0",
    "beberlei/doctrineextensions": "^v1.3.0",
    "gedmo/doctrine-extensions": "^3.0.x-dev"
  },
  "require-dev": {
    "laravel/laravel": "^9.0",
    "phpunit/phpunit": "^9.0"
  }
}
